  "icon": "assets/icon.png",
  "image": "assets/image.png",
  "readMeUrl": "README.md",
  "readMeEndUserUrl": "README_ENDUSER.md",
  "changelogUrl": "CHANGELOG.md",
  "license": "MIT",
  "engines": {
    "das-keyboard-q": "4.1.3"
  },
  "main": "index.js",
  "dependencies": {
    "daskeyboard-applet": "^2.11.5",
    "node-fetch": "^2.6.7"
  },
  "scripts": {
    "test": "jest"
  },
  "qConfig": {
    "geometry": {
      "width": 1,
      "height": 1
    }
  },
  "questions": [
    {
      "key": "latitude",
      "label": "Your Latitude",
      "help": "Enter your latitude (e.g., 40.7128 for New York)",
      "required": true,
      "order": 1,
      "controlType": "textbox",
      "value": "40.7128"
    },
    {
      "key": "longitude",
      "label": "Your Longitude",
      "help": "Enter your longitude (e.g., -74.0060 for New York)",
      "required": true,
      "order": 2,
      "controlType": "textbox",
      "value": "-74.0060"
    }
  ],
  "devDependencies": {
    "jest": "^29.7.0"
  }
